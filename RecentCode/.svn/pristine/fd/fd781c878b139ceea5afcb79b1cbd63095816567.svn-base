<table class="mainTable">
	<colgroup>
		<col width="15%">
		<col width="35%">
		<col width="15%">
		<col width="35%">
	</colgroup>
	<tr> 
		<th>
			ECR 번호
		</th><!-- ECR 제목 -->
		<td colspan="3">
			${data.ecrNumber}
		</td>
	</tr>
	<tr> 
		<th> 
			ECR 제목
		</th><!-- ECR 번호 -->
		<td>
			${data.name}
		</td>
