<!--이곳은은 테이블에 관련된 css-->{ }


<!--t1은 table 에 지정해줍니다.우측+아래에만 선이 나타납니다. class=t1-->{ }

.tb1 {border-right-width:1; border-bottom-width:0;border-top-width:0;border-left-width:0; border-right-color:#999999; border-bottom-color:#999999; border-right-style:solid; border-bottom-style:solid;border-collapse:collapse;}
.tb2 {border-right-width:1; border-bottom-width:1; border-right-color:#999999; border-bottom-color:#cccccc; border-right-style:solid; border-bottom-style:solid;}

<!--t_gray는 td 에 지정해줍니다.좌측+아래 회색 라인입니다. id=t_gray-->{ }
#tb_gray{ font-family: Tahoma,굴림; border-top-width:0; border-right-width:0; border-bottom-width:1; border-left-width:1;  border-bottom-color:#bbbbbb; border-left-color:#bbbbbb; border-style:solid;}
#tb_line {border-top-width:0; border-right-width:0; border-bottom-width:1; border-left-width:0; border-bottom-color:rgb(153,153,153); border-bottom-style:solid;}
#tb_bottom {border-width:1; border-color:#cccccc; border-top-style:none; border-right-style:none; border-bottom-style:solid; border-left-style:none;}


<!--아래는 컬러 시리즈 입니다.td 에 지정해줍니다.색깔별로 id=t_red-->{ }
#tb_red {border-width:1; border-color:#cc3333;background-color:#ff6666; border-style:solid; font-size: 9pt; font-family: 굴림; color: #ffffff;}
#tb_blue {border-width:1; border-color:#667788;background-color:#8899aa; border-style:solid; font-size: 9pt; font-family: 굴림; color: #ffffff;}


<!--메인테이블 정의입니다.-->{ }
<!--t1은 table 에 지정해줍니다.우측+아래 class=t1-->{ }
.t1 {border-right-width:1; border-bottom-width:3; border-right-color:#999999; border-bottom-color:#cccccc; border-right-style:solid; border-bottom-style:solid;}
.t2 {border-right-width:1; border-bottom-width:0; border-right-color:#999999; border-bottom-color:#cccccc; border-right-style:solid; border-bottom-style:solid;}

<!--t_gray는 td 에 지정해줍니다.좌측+아래 회색 라인입니다. id=t_gray-->{ }
#t_gray {border-width:1; border-color:#cccccc; border-top-style:none; border-right-style:none; border-bottom-style:solid; border-left-style:solid;}
#t_bottom {border-width:1; border-color:#cccccc; border-top-style:none; border-right-style:none; border-bottom-style:solid; border-left-style:none;}


<!--아래는 컬러 시리즈 입니다.td 에 지정해줍니다.색깔별로 id=t_red-->{ }
#t_red {padding:1; border-width:3; border-color:#cc3333;background-color:#ff6666; border-style:double; font-size: 9pt; font-family: 굴림; color: #ffffff;}
#t_blue {padding:1; border-width:3; border-color:#667788;background-color:#8899aa; border-style:double; font-size: 9pt; font-family: 굴림; color: #ffffff;}

